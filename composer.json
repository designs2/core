{
  "name": "metamodels/core",
  "description": "MetaModels core",
  "keywords": [
    "contao",
    "metamodels",
    "core"
  ],
  "type": "contao-module",
  "homepage": "http://now.metamodel.me/",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Christian Schiffler",
      "email": "c.schiffler@cyberspectrum.de",
      "homepage": "http://www.cyberspectrum.de",
      "role": "Developer"
    },
    {
      "name": "Stefan Heimes",
      "email": "stefan_heimes@hotmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "mail@metamodel.me",
    "issues": "https://github.com/MetaModels/core/issues",
    "wiki": "http://de.contaowiki.org/MetaModels",
    "irc": "irc://irc.freenode.org/contao.mm",
       "source": "https://github.com/designs2/core"
  },
  "require": {
    "php": ">=5.3",
    "contao/core": ">=3.2,<3.7",
    "contao-community-alliance/composer-plugin": "~2.0",
    "designs2/dc-general": "*",
    "menatwork/contao-multicolumnwizard": "~3.3",
    "designs2/meta-palettes": "*",
    "discordier/justtextwidgets": "*",
    "contao-community-alliance/url-builder": "*",
    "designs2/dependency-container": "*",
    "doctrine/cache": "~1.3"
  },
  "require-dev": {
    "cyberspectrum/contao-toolbox": "*",
    "phpcq/all-tasks": "*",
    "metamodels/base-unit-tests": "*",
    "metamodels/phpunit-contao-database": "*"
  },
  "conflict": {
    "contao/core": "3.4.0"
  },
  "autoload": {
    "psr-0": {
      "MetaModels": "src"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "MetaModels\\Test": "tests"
    }
  },
  "extra": {
    "contao": {
      "sources": {
        "contao": "system/modules/metamodels"
      },
      "transifex": {
        "project": "metamodels",
        "prefix": "core-",
        "languages_cto": "contao/languages",
        "languages_tx": ".tx"
      },
      "runonce": [
        "runonce/runonce.php"
      ]
    },
    "branch-alias": {
      "dev-master": "2.0.x-dev",
      "dev-develop": "2.1.x-dev"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
